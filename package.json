{
  "name": "npm3",
  "version": "1.1.0",
  "description": "Wraps npm v3 so you can run it alongside npm v2",
  "main": "index.js",
  "bin": {
    "npm3": "bin/index.js"
  },
  "scripts": {
    "test": "tape test.js",
    "test": "npm run lint && tape test.js",
    "lint": "standard",
    "start": "npm install --cache-min=0 npm@^3.0.0 && npm test"
  },
  "author": "Tim Oxley <secoif@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "npm": "^3.10.10"
  },
  "devDependencies": {
    "standard": "^8.6.0",
    "tape": "~4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/timoxley/npm3.git"
  },
  "bugs": {
    "url": "https://github.com/timoxley/npm3/issues"
  },
  "homepage": "https://github.com/timoxley/npm3",
  "keywords": [
    "npm",
    "beta",
    "alpha",
    "version",
    "semver",
    "testing",
    "test"
  ]
}
